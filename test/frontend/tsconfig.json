{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "outDir": "built",
    "jsx": "react",
    "noLib": true,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "sourceMap": true
  },
  "exclude": [
    "node_modules"
  ]
}